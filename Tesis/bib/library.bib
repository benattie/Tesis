file = {:home/benattie/Documents/Mendeley Desktop/2014/Acta Materialia/Ung{\'{a}}r et al. - Slip systems and dislocation densities in individual grains of polycrystalline aggregates of plastically deformed CoTi a.pdf:pdf},
isbn = {1359-6454},
issn = {13596454},
journal = {Acta Mater.},
keywords = {B2 intermetallics plasticity,CoTi,CoZr,Dislocation densities,Slip systems},
pages = {264--282},
title = {{Slip systems and dislocation densities in individual grains of polycrystalline aggregates of plastically deformed CoTi and CoZr alloys}},
volume = {71},
year = {2014}
}
