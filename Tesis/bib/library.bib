@article{Kaden2007,
abstract = {The human brain forms a complex neural network with a connectional architecture that is still far from being known in full detail, even at the macroscopic level. The advent of diffusion MR imaging has enabled the exploration of the structural properties of white matter in vivo. In this article we propose a new forward model that maps the microscopic geometry of nervous tissue onto the water diffusion process and further onto the measured MR signals. Our spherical deconvolution approach completely parameterizes the fiber orientation density by a finite mixture of Bingham distributions. In addition, we define the term anatomical connectivity, taking the underlying image modality into account. This neurophysiological metric may represent the proportion of the nerve fibers originating in the source area which intersect a given target region. The specified inverse problem is solved by Bayesian statistics. Posterior probability maps denote the probability that the connectivity value exceeds a chosen threshold, conditional upon the noisy observations. These maps allow us to draw inferences about the structural organization of the cerebral cortex. Moreover, we will demonstrate the proposed approach with diffusion-weighted data sets featuring high angular resolution. ?? 2007 Elsevier Inc. All rights reserved.},
author = {Kaden, Enrico and Kn{\"{o}}sche, Thomas R. and Anwander, Alfred},
doi = {10.1016/j.neuroimage.2007.05.012},
file = {:home/benattie/Documents/Mendeley Desktop/2007/NeuroImage/Kaden, Kn{\"{o}}sche, Anwander - Parametric spherical deconvolution Inferring anatomical connectivity using diffusion MR imaging.pdf:pdf},
isbn = {1053-8119 (Print)},
issn = {10538119},
journal = {Neuroimage},
keywords = {Anatomical connectivity,Bayesian statistics,Diffusion MR imaging,Finite mixture of Bingham distributions,Parametric spherical deconvolution},
number = {2},
pages = {474--488},
pmid = {17596967},
title = {{Parametric spherical deconvolution: Inferring anatomical connectivity using diffusion MR imaging}},
volume = {37},
year = {2007}
}
