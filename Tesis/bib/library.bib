@article{Wu2007,
abstract = {A new Taylor-type polycrystalline model has been developed to simulate the evolution of crystallographic texture and the anisotropic stress-strain response during large plastic deformation of high purity $\alpha$-titanium at room temperature. Crystallographic slip, deformation twinning and slip inside twinned regions were all considered as contributing mechanisms for the plastic strain in the model. This was accomplished by treating the dominant twin systems in a given crystal as independent grains once the total twin volume fraction in that crystal reached a predetermined saturation value. The newly formed grains were allowed to independently undergo further slip and the concomitant lattice rotation, but further twinning was prohibited. New descriptions have been established for slip and twin hardening and the complex coupling between them. Good predictions were obtained for the overall anisotropic stress-strain response and texture evolution in three different monotonic deformation paths on annealed, initially textured samples of high purity $\alpha$-titanium. {\textcopyright} 2006 Acta Materialia Inc.},
author = {Wu, Xianping and Kalidindi, Surya R. and Necker, Carl and Salem, Ayman A.},
doi = {10.1016/j.actamat.2006.08.034},
file = {:home/benattie/Documents/Mendeley Desktop/2007/Acta Materialia/Wu et al. - Prediction of crystallographic texture evolution and anisotropic stress-strain curves during large plastic strains in high.pdf:pdf},
isbn = {1359-6454},
issn = {13596454},
journal = {Acta Mater.},
keywords = {Anisotropic,Hardening,Plastic deformation,Texture,Twinning},
number = {2},
pages = {423--432},
title = {{Prediction of crystallographic texture evolution and anisotropic stress-strain curves during large plastic strains in high purity $\alpha$-titanium using a Taylor-type crystal plasticity model}},
volume = {55},
year = {2007}
}
