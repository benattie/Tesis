isbn = {9783037857663},
issn = {13596454},
journal = {Acta Mater.},
keywords = {Deformation twinning,EBSD,Neutron diffraction,Short range ordering,Ti-Al binary alloys},
pages = {341--351},
publisher = {Elsevier Ltd},
title = {{The effect of aluminium on twinning in binary alpha-titanium}},
url = {http://dx.doi.org/10.1016/j.actamat.2015.09.048},
volume = {103},
year = {2016}
}
