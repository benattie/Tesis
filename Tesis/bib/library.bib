booktitle = {Microsc. Microanal.},
doi = {10.1017/S1431927611000055},
file = {:home/benattie/Documents/Mendeley Desktop/2011/Microscopy and microanalysis the official journal of Microscopy Society of America, Microbeam Analysis Society, Microscopical Society of Ca./Wright, Nowell, Field - A review of strain analysis using electron backscatter diffraction.pdf:pdf},
