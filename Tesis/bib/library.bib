file = {:home/benattie/Documents/Mendeley Desktop/2016/Acta Materialia/C{\'{i}}zek et al. - Structural characterization of ultrafine-grained interstitial-free steel prepared by severe plastic deformation.pdf:pdf},
issn = {13596454},
journal = {Acta Mater.},
keywords = {Interstitial free steel,Positron annihilation,Severe plastic deformation,Ultrafine-grained materials,X-ray diffraction},
pages = {258--272},
title = {{Structural characterization of ultrafine-grained interstitial-free steel prepared by severe plastic deformation}},
volume = {105},
year = {2016}
}
